{
  "private": false,
  "name": "@defichain/jellyfish",
  "version": "0.0.0",
  "description": "A collection of TypeScript/JavaScript tools and libraries for DeFiChain to build decentralized finance on Bitcoin",
  "keywords": [
    "DeFiChain",
    "JavaScript",
    "API"
  ],
  "repository": "DeFiCh/jellyfish",
  "bugs": "https://github.com/DeFiCh/jellyfish/issues",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "contributors": [
    {
      "name": "DeFiChain Foundation",
      "email": "engineering@defichain.com",
      "url": "https://defichain.com/"
    },
    {
      "name": "DeFiChain Jellyfish Developers"
    }
  ],
  "main": "dist/jellyfish.cjs.js",
  "module": "dist/jellyfish.esm.js",
  "browser": "dist/jellyfish.umd.js",
  "types": "dist/jellyfish.d.ts",
  "files": [
    "dist"
  ],
  "targets": {
    "browser": {
      "includeNodeModules": {
        "@defichain/jellyfish-core": true,
        "@defichain/jellyfish-rpc": true
      },
      "engines": {
        "browsers": "> 0.25%"
      }
    }
  },
  "engines": {
    "node": ">=12.x",
    "electron": ">=10.x"
  },
  "scripts": {
    "build": "parcel build src/jellyfish.ts",
    "publish:next": "npm publish --tag next",
    "publish:latest": "npm publish --tag latest"
  },
  "dependencies": {
    "@defichain/jellyfish-core": "0.0.0",
    "@defichain/jellyfish-rpc": "0.0.0"
  },
  "bundledDependencies": [
    "@defichain/jellyfish-core",
    "@defichain/jellyfish-rpc"
  ],
  "devDependencies": {
    "@defichain/testcontainers": "0.0.0",
    "@parcel/transformer-typescript-types": "2.0.0-beta.1",
    "parcel": "2.0.0-beta.1",
    "typescript": "^4.2.2"
  }
}
